//To perform arthimetic operations using functions
#include<stdio.h>
int add(int,int);
int multi(int,int);
int sub(int,int);
float div(int,int);
int main()
{
	int a,b;
	scanf("%d%d",&a,&b);
	printf("Addition=%d\n",add(a,b));
	printf("Subtraction=%d\n",sub(a,b));
	printf("Multiplication=%d\n",multi(a,b));
	printf("Division=%.2f\n",div(a,b));
}
int add(int a,int b)
{
	return a+b;
}
int sub(int a,int b)
{
	return a-b;
}
int multi(int a,int b)
{
	return a*b;
}
float div(int a,int b)
{
	return a/b;
}

